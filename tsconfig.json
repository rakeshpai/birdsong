{
  "include": [
    "./src",
    "./vite.config.js"
  ],
  "exclude": [],
  "compilerOptions": {
    "rootDir": "./",
    "outDir": "dist/server/esm",
    "target": "esnext",
    "module": "esnext",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "allowSyntheticDefaultImports": true,
    "sourceMap": true,
    "declaration": true,
    "types": [
      "vitest"
    ]
  },
  "lib": [
    "esnext"
  ],
}